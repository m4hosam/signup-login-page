<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="css/style3.css">
    <title>Data</title>
</head>

<body>
    <h1><span class="yellow">User Data</pan>
    </h1>

    <table class="container">
        <thead>
            <tr>
                <th>
                    <h1>Name</h1>
                </th>
                <th>
                    <h1>Email</h1>
                </th>
                <th>
                    <h1>password</h1>
                </th>
            </tr>
        </thead>
        <tbody>
            <% for(let i=0 ; i < data.length ; i++){ %>
                <tr>
                    <td>
                        <%= data[i].name %>
                    </td>
                    <td>
                        <%= data[i].email %>
                    </td>
                    <td>
                        <%= data[i].password %>
                    </td>
                </tr>
                <% } %>
        </tbody>
    </table>

</body>

</html>